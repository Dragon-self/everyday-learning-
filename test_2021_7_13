int main()
{
	int day = 0;
	scanf("%d", &day);
	switch (day)//(括号内一定是整形表达式)
	{
	case 1://case 后面一定是整形常量表达式
	case 2:
	case 3:
	case 4:
	case 5:
		printf("工作日\n");
		break;
	case 6:
	case 7:
		printf("休息日\n");
		break;
	default:
		printf("输入错误\n");
		break;
	}
	return 0;
}


