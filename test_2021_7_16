#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
	int i = 0;
	//  初始化   判断   调整
	for (i = 1; i <= 10; i++)
	{
		if (i == 5)
			continue;//1234678910//i++仍会执行
		printf("%d", i);
	}
	return 0;
}

