int i;//全局变量-不初始化-默认是0
int main()
{
	i--;
	//11111111111111111111111111111111--i
	if (i > sizeof(i))//sizeof()--计算变量/类型所占内存的大小>=0--无符号数（二进制）
	{
		printf(">\n");
	}
	else
	{
		printf("<\n");
	}
	return 0;
}
