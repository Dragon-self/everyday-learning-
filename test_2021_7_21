int Max(int x, int y)//定义函数
{
	if (x > y)
		return x;
	else
		return y;
}

int main()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);
	int max = Max(a, b);//函数使用
	printf("max=%d\n", max);
}
